<configuration xmlns="http://fedorahosted.org/xmvn/CONFIG/2.0.0">
  <properties/>
  <buildSettings>
    <debug>false</debug>
    <skipTests>false</skipTests>
  </buildSettings>
  <resolverSettings>
    <debug>false</debug>
    <ignoreDuplicateMetadata>true</ignoreDuplicateMetadata>
  </resolverSettings>
  <installerSettings>
    <debug>false</debug>
  </installerSettings>

  <repositories>
    <repository>
      <id>resolve</id>
      <type>compound</type>
      <configuration>
        <repositories/>
      </configuration>
    </repository>
    <repository>
      <id>install</id>
      <type>compound</type>
      <configuration>
        <repositories/>
      </configuration>
    </repository>
    <repository>
      <id>install-raw-pom</id>
      <type>compound</type>
      <configuration>
        <repositories/>
      </configuration>
    </repository>
    <repository>
      <id>install-effective-pom</id>
      <type>compound</type>
      <configuration>
        <repositories/>
      </configuration>
    </repository>
  </repositories>
</configuration>
