<project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>xmvn.its</groupId>
  <artifactId>plugin-alias</artifactId>
  <version>0.0.0-SNAPSHOT</version>
  <build>
    <plugins>
      <plugin>
        <groupId>alias-test</groupId>
        <artifactId>aliased-component-metadata</artifactId>
        <version>DUMMY</version>
        <executions>
          <execution>
            <goals>
              <goal>generate-metadata</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <generatedMetadata>component-metadata-test.xml</generatedMetadata>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
